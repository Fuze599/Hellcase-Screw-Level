{
  "manifest_version": 3,
  "name": "Hellcase Average Calculator",
  "version": "1.01",
  "description": "Want to know which Hellcase weapon crate is worth it? This addon is for you",
  "permissions": [
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://hellcase.com/*"], 
      "js": ["content.js"]
    }
  ],
  "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
}